# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/docroot/core/
/docroot/modules/contrib/
/docroot/themes/contrib/
/docroot/profiles/contrib/
/docroot/libraries/
/config/ignored/*

# Ignore sensitive information
/docroot/sites/*/settings.local.php

# Ignore Drupal's file directory
/docroot/sites/*/files/
/docroot/sites/*/public/
/docroot/sites/*/private/
/docroot/sites/*/files-public/
/docroot/sites/*/files-private/

# Ignore paths that contain temporary files.
/docroot/sites/*/translations/
/docroot/sites/*/tmp/

# Ignore SimpleTest multi-site environment.
/docroot/sites/simpletest

/composer.lock

# Ignore the build caches
.sass-cache

# Editor and IDE files
*.un~
*.kate-swp
.project
.buildpath
.settings*
.idea

# OS generated files
.DS_Store*
ehthumbs.db
Icon?
Thumbs.db
._*

# Archives and binaries
*.tar
*.gz
*.bz2
*.7z
*.xz
*.zip
*.rar
*.tgz
*.iso
*.jar
*.dmg
*.exe
*.so
*.dll

# Logs and databases
*.log
*.sql

